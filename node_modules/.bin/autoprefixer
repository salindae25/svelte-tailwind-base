#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/autoprefixer@10.4.4_postcss@8.4.12/node_modules/autoprefixer/bin/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/autoprefixer@10.4.4_postcss@8.4.12/node_modules/autoprefixer/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/autoprefixer@10.4.4_postcss@8.4.12/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/node_modules:/Users/sal234/Documents/Personal/Projects/node_modules:/Users/sal234/Documents/Personal/node_modules:/Users/sal234/Documents/node_modules:/Users/sal234/node_modules:/Users/node_modules:/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/autoprefixer/bin/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/autoprefixer/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/autoprefixer@10.4.4_postcss@8.4.12/node_modules/autoprefixer/bin/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/autoprefixer@10.4.4_postcss@8.4.12/node_modules/autoprefixer/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/autoprefixer@10.4.4_postcss@8.4.12/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/node_modules:/Users/sal234/Documents/Personal/Projects/node_modules:/Users/sal234/Documents/Personal/node_modules:/Users/sal234/Documents/node_modules:/Users/sal234/node_modules:/Users/node_modules:/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/autoprefixer/bin/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/autoprefixer/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../autoprefixer/bin/autoprefixer" "$@"
else
  exec node  "$basedir/../autoprefixer/bin/autoprefixer" "$@"
fi
