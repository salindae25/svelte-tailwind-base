#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/detective@5.2.0/node_modules/detective/bin/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/detective@5.2.0/node_modules/detective/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/detective@5.2.0/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/node_modules:/Users/sal234/Documents/Personal/Projects/node_modules:/Users/sal234/Documents/Personal/node_modules:/Users/sal234/Documents/node_modules:/Users/sal234/node_modules:/Users/node_modules:/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/tailwindcss@3.0.23_autoprefixer@10.4.4/node_modules/tailwindcss/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/tailwindcss@3.0.23_autoprefixer@10.4.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/detective@5.2.0/node_modules/detective/bin/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/detective@5.2.0/node_modules/detective/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/detective@5.2.0/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/node_modules:/Users/sal234/Documents/Personal/Projects/node_modules:/Users/sal234/Documents/Personal/node_modules:/Users/sal234/Documents/node_modules:/Users/sal234/node_modules:/Users/node_modules:/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/tailwindcss@3.0.23_autoprefixer@10.4.4/node_modules/tailwindcss/node_modules:/Users/sal234/Documents/Personal/Projects/github.com/svelte-tailwind-base/node_modules/.pnpm/tailwindcss@3.0.23_autoprefixer@10.4.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../detective@5.2.0/node_modules/detective/bin/detective.js" "$@"
else
  exec node  "$basedir/../../../../../detective@5.2.0/node_modules/detective/bin/detective.js" "$@"
fi
